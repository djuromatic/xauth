<form autocomplete="off" action="/interaction/<%= uid %>/signup" " method="post">
  <input required type="text" name="email" placeholder="Enter email" />
  <input required type="password" name="password" placeholder="Enter password" />
  <input required type="password" name="repeatedPassword" placeholder="Repeat password" />

  <input required type="text" name="username" placeholder="Enter username" />
  <input required type="date" name="dateOfBirth" placeholder="Enter date of birth" />
  <select id="gender">

    <option value="male">Male</option>
    <option value="female">Female</option>
    <option value="other" >Other</option>

  
  </select>

  <button type="submit" class="signup signup-submit">Sign-Up</button>
</form>
