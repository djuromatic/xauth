{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "allowJs": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
    // "paths": {
    //   "@/*": ["./src/*"]
    // }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
